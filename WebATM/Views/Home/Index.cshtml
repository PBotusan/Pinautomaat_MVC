<div class="row" >
    <div class="col-md-6 margin-top-20">
        <a href="@Url.Action("Withdrawl", "Transactions",new { checkingAccountId = ViewBag.CheckingAccountId })" class="btn btn-primary  btn-lg btn-block"><span class="fa fa-arrow-down"></span> Overboeken </a>
        <a href="@Url.Action("Withdrawl", "Transactions", new { checkingAccountId = ViewBag.CheckingAccountId})" class="btn btn-primary btn-lg btn-block"><span class="fa fa-arrow-down"></span> Geld pinnen </a>
